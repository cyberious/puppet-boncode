<Settings>
    <Server><%= @host %></Server>
    <Port><%= @port %></Port>
    <PacketSize><%= @packetSize %></PacketSize>
    <EnableHTTPStatusCodes><%= @enableHTTPStatusCodes %></EnableHTTPStatusCodes>
    <EnableHeaderDataSupport><%= @enableHeaderDataSupport %></EnableHeaderDataSupport>
    <EnableRemoteAdmin><%= @enableRemoteAdmin %></EnableRemoteAdmin>
    <AllowEmptyHeaders><%= @allowEmptyHeaders %></AllowEmptyHeaders>
    <MaxConnections><%= @maxConnections %></MaxConnections>
    <FlushThreshold><%= @flushThreshold %></FlushThreshold>
    <WriteTimeOut><%= @writeTimeOut %></WriteTimeOut>
    <LogLevel><%= @logLevel %></LogLevel>
    <LogDir><%= @logDir %></LogDir>
</Settings>

